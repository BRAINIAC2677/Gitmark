{
  "name": "Gitmark",
  "description": "A simple, fast, and powerful private github repo starring tool",
  "version": "0.1.0",
  "author": "Asif Azad",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "permissions": ["storage"],
  "icons": {
    "16": "icons/logo16.png",
    "32": "icons/logo32.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  }
}
